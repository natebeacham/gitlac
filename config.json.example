{
	"repo": "/path/to/repo",
	
	"limit": 100000,

	"prefix": "refs/remotes/origin/",

	"refs": [
		"master"
	],

	"exts": [
		".py",
		".js",
		".css",
		".html"
	],

	"ignore": [
		"jquery",
		"underscore",
		"backbone"
	],

	"watch": [
		"project/",
		"src/"
	],

	"merge": [
		"Merge branch",
		"Merge remote"
	]
}
